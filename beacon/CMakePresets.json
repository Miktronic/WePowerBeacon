{
    "version": 2,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 20
    },
    "configurePresets": [
        {
            "name": "build",
            "displayName": "Build for WP Rev1",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "NCS_TOOLCHAIN_VERSION": "NONE",
                "BOARD": "wp_rev1",
                "CONF_FILE": "$env{ZEPHYR_BASE}/d:/WePowerBeacon/beacon/prj.conf"
            }
        }
    ]
}